{
  "name": "digital-hub",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:start": "DEBUG=gov-starter-server* nodemon server.js | bunyan -o short",
    "build": "npm run css-build",
    "build-production": "webpack --config webpack.config.js --mode=production",
    "css-autoprefix": "postcss --use autoprefixer -b 'last 5 versions' < assets/stylesheets/application.css | postcss --use cssnano > assets/stylesheets/application.min.css",
    "css-build": "./bin/build-css",
    "clean": "rm -rf public/* .port.tmp *.log build/* uploads/* test-results.xml",
    "lint": "eslint . --cache --max-warnings 0",
    "test": "NODE_ENV=test mocha test",
    "cy:run": "cypress run --browser chrome",
    "cy:open": "cypress open",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:run": "start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cy:open",
    "record-build-info": "node ./bin/record-build-info",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|scss|md)\"",
    "verify": "npm run lint && npm run prettier -- --list-different && npm run test",
    "format": "npm run prettier -- --write"
  },
  "engines": {
    "node": ">= 8.10.0",
    "npm": ">=5.6.0"
  },
  "nodemonConfig": {
    "ignore": [
      ".circleci/*",
      "migrations/*",
      "node_modules/*",
      "test/*",
      "build-info.json"
    ],
    "delay": "2500"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run verify"
    }
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "bunyan-format": "^0.2.1",
    "bunyan-request-logger": "^2.0.1",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.3",
    "cookie-session": "^2.0.0-beta.3",
    "date-fns": "^1.30.1",
    "dotenv": "^6.2.0",
    "express": "~4.16.4",
    "express-request-id": "^1.4.1",
    "express-session": "^1.15.6",
    "govuk-elements-sass": "^3.1.3",
    "govuk-frontend": "^2.0.0",
    "govuk_frontend_toolkit": "^7.6.0",
    "govuk_template_jinja": "^0.24.1",
    "helmet": "^3.15.0",
    "jplayer": "^2.9.2",
    "jquery": "^3.3.1",
    "memorystore": "^1.6.0",
    "mkdirp": "^0.5.1",
    "mustache": "^3.0.1",
    "node-sass-middleware": "0.11.0",
    "nunjucks": "^3.1.6",
    "ramda": "^0.26.1",
    "sanitizer": "^0.1.3",
    "superagent": "^4.1.0",
    "video.js": "^7.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "autoprefixer": "^9.4.10",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-string": "^1.5.0",
    "cssnano": "^4.1.10",
    "cssnano-cli": "^1.0.5",
    "cypress": "^3.2.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.14.0",
    "from-html": "^0.3.3",
    "husky": "^1.3.1",
    "is-ci": "^2.0.0",
    "jsdom": "^13.2.0",
    "lint-staged": "^8.1.4",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.14",
    "postcss-cli": "^6.1.2",
    "prettier": "^1.16.4",
    "puppeteer": "^1.11.0",
    "sass-middleware": "^0.0.3",
    "sinon": "^7.2.2",
    "start-server-and-test": "^1.7.12",
    "superagent-nock": "^0.4.0",
    "supertest": "^3.3.0",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  }
}
