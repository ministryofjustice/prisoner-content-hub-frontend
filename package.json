{
  "name": "prisoner-content-hub-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
    "start": "node server.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:start": "DEBUG=gov-starter-server* nodemon server.js",
    "build": "npm run css-build",
    "css-build": "./bin/build-css",
    "clean": "rm -rf public/* .port.tmp *.log build/* uploads/* test-results.xml",
    "lint": "eslint . --cache --max-warnings 0",
    "test": "jest",
    "clear_jest": "jest --clearCache",
    "start:e2e": "export $(cat feature.env) && node server.js",
    "test:e2e:run": "start-server-and-test start:e2e http://localhost:3000 test:e2e:ci",
    "test:e2e:ci": "cypress run --spec cypress/features/timetable.feature",
    "test:e2e:ui": "cypress open",
    "record-build-info": "node ./bin/record-build-info",
    "prettier": "prettier \"**/*.+(js|jsx|json|css|scss|md)\"",
    "verify": "npm run lint && npm run prettier -- --list-different && npm run test",
    "format": "npm run prettier -- --write",
    "security_audit": "npx audit-ci --config audit-ci.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ministryofjustice/prisoner-content-hub-frontend/"
  },
  "engines": {
    "node": ">= 14",
    "npm": ">=6.14.8"
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)"
    ]
  },
  "dependencies": {
    "@sentry/node": "^6.7.1",
    "axios": "0.21.1",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0-rc.1",
    "date-fns": "^2.22.1",
    "dotenv": "^10.0.0",
    "drupal-jsonapi-params": "^1.2.2",
    "elastic-builder": "^2.16.0",
    "express": "~4.17.1",
    "express-request-id": "^1.4.1",
    "govuk-frontend": "^3.12.0",
    "helmet": "^4.6.0",
    "jquery": "^3.6.0",
    "jsona": "^1.9.2",
    "jsonwebtoken": "^8.5.1",
    "mkdirp": "^1.0.4",
    "morgan": "^1.10.0",
    "nocache": "^3.0.0",
    "node-sass-middleware": "0.11.0",
    "nunjucks": "^3.2.3",
    "passport": "^0.4.1",
    "passport-azure-ad-oauth2": "0.0.4",
    "ramda": "^0.27.1",
    "redis": "^3.1.2",
    "uuid": "^8.3.2",
    "video.js": "^7.12.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@sinonjs/fake-timers": "^7.1.2",
    "babel-eslint": "^10.1.0",
    "chance": "^1.1.7",
    "cheerio": "^1.0.0-rc.10",
    "cypress": "^7.5.0",
    "cypress-cucumber-preprocessor": "^4.1.2",
    "cypress-multi-reporters": "^1.5.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.23.4",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "jsdom": "^16.6.0",
    "lint-staged": "^11.0.0",
    "mocha-junit-reporter": "^2.0.0",
    "nock": "^13.1.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "start-server-and-test": "^1.12.5",
    "supertest": "^6.1.3"
  }
}
