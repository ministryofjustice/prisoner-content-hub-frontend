{
  "name": "prisoner-content-hub-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run css-build",
    "css-build": "./bin/build-css",
    "clean": "rm -rf public/* .port.tmp *.log build/* uploads/* test-results.xml",
    "lint": "eslint . --cache --max-warnings 0",
    "test": "jest --runInBand",
    "clear_jest": "jest --clearCache",
    "test:e2e:ci": "npm run start-ci --prefix ./e2e",
    "test:e2e:ui": "npm run start-ui --prefix ./e2e",
    "record-build-info": "node ./bin/record-build-info",
    "prettier": "prettier \"**/*.+(js|jsx|json|css|scss|md)\"",
    "verify": "npm run lint && npm run prettier -- --list-different && npm run test",
    "format": "npm run prettier -- --write",
    "security_audit": "npx audit-ci --config audit-ci.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ministryofjustice/prisoner-content-hub-frontend/"
  },
  "engines": {
    "node": ">= 16",
    "npm": ">= 7"
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)"
    ]
  },
  "dependencies": {
    "@ministryofjustice/frontend": "^1.0.0",
    "@sentry/node": "^6.15.0",
    "@sentry/tracing": "^6.15.0",
    "axios": "0.24.0",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0-rc.1",
    "cookies": "^0.8.0",
    "date-fns": "^2.25.0",
    "dotenv": "^10.0.0",
    "drupal-jsonapi-params": "^1.2.2",
    "express": "~4.17.1",
    "express-request-id": "^1.4.1",
    "govuk-frontend": "^3.14.0",
    "helmet": "^4.6.0",
    "jquery": "^3.6.0",
    "jsona": "^1.9.3",
    "jsonwebtoken": "^8.5.1",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "nocache": "^3.0.1",
    "nunjucks": "^3.2.3",
    "passport": "^0.5.0",
    "passport-azure-ad-oauth2": "0.0.4",
    "ramda": "^0.27.1",
    "redis": "^3.1.2",
    "uuid": "^8.3.2",
    "video.js": "^7.17.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@sinonjs/fake-timers": "^8.1.0",
    "cheerio": "^1.0.0-rc.10",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jsdom": "^18.1.0",
    "lint-staged": "^12.0.2",
    "nock": "^13.2.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "start-server-and-test": "^1.14.0",
    "supertest": "^6.1.6"
  }
}
