{
  "name": "gov-starter",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "start:dev": "npm run build && DEBUG=gov-starter-server* nodemon server.js | bunyan -o short",
    "build": "npm run css-build",
    "css-build": "./bin/build-css",
    "clean": "rm -rf public/* .port.tmp *.log build/* uploads/* test-results.xml",
    "lint": "eslint . --cache --max-warnings 0",
    "test": "NODE_ENV=test mocha test",
    "test:e2e": "NODE_ENV=test mocha test-e2e",
    "record-build-info": "node ./bin/record-build-info",
    "verify": "npm run lint && npm run test"
  },
  "engines": {
    "node": ">= 8.10.0",
    "npm": ">=5.6.0"
  },
  "nodemonConfig": {
    "ignore": [
      ".circleci/*",
      "migrations/*",
      "node_modules/*",
      "test/*",
      "build-info.json"
    ],
    "delay": "2500"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "bunyan-format": "^0.2.1",
    "bunyan-request-logger": "^2.0.0",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.3",
    "cookie-session": "^2.0.0-beta.3",
    "dotenv": "^6.2.0",
    "express": "~4.16.4",
    "express-request-id": "^1.4.1",
    "express-session": "^1.15.6",
    "govuk-elements-sass": "^3.1.3",
    "govuk-frontend": "^2.0.0",
    "govuk_frontend_toolkit": "^7.6.0",
    "govuk_template_jinja": "^0.24.1",
    "helmet": "^3.15.0",
    "jplayer": "^2.9.2",
    "jquery": "^3.3.1",
    "memorystore": "^1.6.0",
    "mkdirp": "^0.5.1",
    "mustache": "^3.0.1",
    "node-sass-middleware": "0.11.0",
    "nunjucks": "^3.1.6",
    "ramda": "^0.26.1",
    "superagent": "^4.1.0",
    "video.js": "^7.4.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-string": "^1.5.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "nodemon": "^1.18.9",
    "puppeteer": "^1.11.0",
    "sass-middleware": "^0.0.3",
    "sinon": "^7.2.2",
    "superagent-nock": "^0.4.0",
    "supertest": "^3.3.0"
  }
}
